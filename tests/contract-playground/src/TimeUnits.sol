pragma solidity 0.8.20;


contract TimeUnits {

    uint256 private constant SECOND = 1;
    uint256 private constant MINUTE = 60;
    uint256 private constant HOUR = 60 * 60;
    uint256 private constant DAY = 24 * 60 * 60;
    uint256 private constant WEEKS = 7 * 24 * 60 * 60;
    uint256 private constant MONTH = 30 * 7 * 24 * 60 * 60;
    uint256 private constant YEAR = 365 * 24 * 60 * 60;

    uint256 private constant SECOND_= 1 seconds;
    uint256 private constant MINUTE_= 1 minutes;
    uint256 private constant HOUR_= 1 hours;
    uint256 private constant DAY_= 1 days;
    uint256 private constant WEEKS_= 1 weeks;

}
